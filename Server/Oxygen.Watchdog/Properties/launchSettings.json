{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "Oxygen.Watchdog": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "healthchecks",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "applicationUrl": "https://localhost:5009;http://localhost:5008"
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/healthchecks",
      "publishAllPorts": true,
      "useSSL": true
    }
  }
}